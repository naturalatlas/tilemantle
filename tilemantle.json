{
	"port": 8080,
	"store": {
		"adapter": "sqlite",
		"file": ":memory:"
	},
	"concurrency": 5,
	"delay_between_tiles": 50,
	"idle_delay": 250,
	"upload_limit": "5mb",
	"preset_defaults": {
		"retries": 5,
		"retry_delay": 1000,
		"minZoom": 6,
		"maxZoom": 15
	},
	"presets": {
		"basemap": {
			"title": "Basemap",
			"requests": [
				{"url": "https://tiles.naturalatlas.com/topo/{z}/{x}/{y}/t.pbf"},
				{"url": "https://tiles.naturalatlas.com/basemap/{z}/{x}/{y}/t.png"},
				{"url": "https://tiles.naturalatlas.com/basemap/{z}/{x}/{y}/t@2x.png"},
				{"url": "https://tiles.naturalatlas.com/topo/{z}/{x}/{y}/t.pbf"}
			],
			"buffer": {
				"15": 1,
				"14": 1,
				"13": 1,
				"12": 1,
				"11": 1,
				"10": 1,
				"9": 1,
				"8": 1,
				"7": 1,
				"6": 1
			}
		}
	},
	"display_layers": [
		{"title": "OpenStreetMap", "url_1x": "http://{s}.tile.osm.org/{z}/{x}/{y}.png", "attribution": "&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors"},
		{"title": "Natural Atlas", "url_1x": "http://tiles.naturalatlas.com/basemap/{z}/{x}/{y}/t.png", "url_2x": "http://tiles.naturalatlas.com/basemap/{z}/{x}/{y}/t@2x.png", "minZoom": 6, "maxZoom": 15}
	]
}